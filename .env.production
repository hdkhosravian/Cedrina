# Project Metadata
PROJECT_NAME=cedrina
VERSION=0.1.0
APP_ENV=staging
DEBUG=false

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
RELOAD=false

# Logging
LOG_LEVEL=INFO
LOG_JSON=true

# Security
SECRET_KEY=your-staging-secret-key-12345678901234567890123456789012
ALLOWED_ORIGINS=https://staging.example.com

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_staging_postgres_password
POSTGRES_DB=cedrina_staging
POSTGRES_HOST=external.db.host
POSTGRES_PORT=5432
POSTGRES_SSL_MODE=require
POSTGRES_POOL_SIZE=10
POSTGRES_MAX_OVERFLOW=20
POSTGRES_POOL_TIMEOUT=30
CEDRINA_DEV_PASSWORD=your_staging_cedrina_password
DATABASE_URL=postgresql+psycopg2://postgres:your_staging_postgres_password@external.db.host:5432/cedrina_staging?sslmode=require

# Redis
REDIS_HOST=external.redis.host
REDIS_PORT=6379
REDIS_PASSWORD=your_staging_redis_password
REDIS_SSL=true
REDIS_URL=rediss://:your_staging_redis_password@external.redis.host:6379/0